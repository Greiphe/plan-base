                          <option value="Operacional">Operacional</option>
                          <option value="Não Operacional">Não Operacional</option>
                        </select>
                      </div>
                    </div>

                    <div class="group1-2 d-flex flex-column">
                      <div class="input-despesa">
                        <input class="inpt-despesa-el inpt-despesa-el-data" required="" id="dataInput" type="text"
